<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="368.0" prefWidth="241.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Server.UsersWindowController">
   <children>
      <TableView fx:id="tableView" layoutX="20.0" layoutY="120.0" onMouseClicked="#userClickedOnTable" prefHeight="200.0" prefWidth="199.0">
        <columns>
          <TableColumn fx:id="user" prefWidth="198.0" text="Users" />
        </columns>
      </TableView>
      <Button layoutX="148.0" layoutY="82.0" mnemonicParsing="false" onAction="#changeSceneToNewUser" text="New User" />
      <Button fx:id="delete" layoutX="166.0" layoutY="330.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete" />
      <Pane prefHeight="71.0" prefWidth="241.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="73.0" layoutY="17.0" text="USERS" textFill="WHITE">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
