<AnchorPane id="AnchorPane" prefHeight="347.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Server.PatientsWindowController">
   <children>
      <TableView fx:id="tableView" layoutX="54.0" layoutY="98.0" onMouseClicked="#userClickedOnTable" prefHeight="185.0" prefWidth="382.0">
        <columns>
          <TableColumn fx:id="date" prefWidth="125.0" text="Date" />
          <TableColumn fx:id="patientID" minWidth="7.0" prefWidth="135.0" text="Patient ID" />
            <TableColumn fx:id="patientName" prefWidth="121.0" text="Patient Name" />
        </columns>
      </TableView>
      <Button fx:id="detailedPersonViewButton" layoutX="391.0" layoutY="306.0" mnemonicParsing="false" onAction="#changeSceneToDetailedPersonView" text="View" />
      <Pane layoutX="-1.0" prefHeight="71.0" prefWidth="489.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="98.0" layoutY="17.0" text="HOSPITAL  PATIENTS" textFill="WHITE">
               <font>
                  <Font size="31.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>     
</AnchorPane>
